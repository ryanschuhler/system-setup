[user]
	name = ryanschuhler
	email = ryan.schuhler@liferay.com
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
	aliases = "!git config -l | grep alias | cut -c 7-"
	br = branch
	bra = branch -a
	brd = branch -d
	brD = branch -D
	ci = commit
	cia = commit --amend
	co = checkout
	cob = checkout -b
	cl = clone
	cp = cherry-pick
	dc = diff --cached
	log = log --decorate --graph --date=short
	l = log
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
	re = remote
	rea = remote add
	ri = rebase -i
	rih = rebase -i HEAD~10
	sa = stash apply
	sl = stash list
	ss = stash save
	slog = log --pretty=oneline --abbrev-commit
	st = status
	po = push origin
	pr = pull --rebase
	pru = pull --rebase upstream
	
